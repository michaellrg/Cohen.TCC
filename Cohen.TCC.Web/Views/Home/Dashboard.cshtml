@{
    Layout = "~/Views/Shared/_DashboardDados.cshtml";
    ViewBag.Title = "Dashboard";

}



<div ng-app="veiculoApp">
    <div ng-controller="veiculoCtrl">
        <div class="col-lg-12">
            <table class="table table-striped">
                <tr>
                    <th>ID Ticket</th>
                    <th>Nome</th>
                    <th>Data de Entrada</th>
                    <th>Data de Saída</th>
                    <th>Preço</th>
                </tr>
                <tr ng-repeat="ticket in tickets">
                    <td> {{ticket.id_ticket}} </td>
                    <td> {{ticket.nm_cliente}} </td>
                    <td> {{ticket.dt_entrada.replace('/Date(','').replace(')/','') | date:"dd/MM/yyyy h:mma"}} </td>
                    <td> {{ticket.dt_saida.replace('/Date(','').replace(')/','') | date:"dd/MM/yyyy h:mma"}} </td>
                    <td> {{ticket.nu_valor}} </td>
                </tr>
                
                <tr>
                    
                     <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    @if (ViewBag.TipoCliente != 1)
                    {
                    <td ><br /><label> <b>Total :</b> {{ getTotal() }}</label></td>
                    }
                    else
                    {
                        <td><br /><label> <b>Total :</b> 150,00</label></td>
                    }

                </tr>
            </table>
        </div>
    </div>
</div>
